        <title>IFrame</title>
        <style>
            body,
            html {
                margin: 0;
                padding: 0;
            }

            iframe {
                width: 1px;
                min-width: 100%;
                border: none;
                height: 95vh;
                overflow: auto;
            }
            .opentab,
            .opentab:visited {
                text-decoration: none;
                color: #333;
                font-size: 0.8em;
            }

            .opentab:hover,
            .opentab:active {
                text-decoration: underline;
            }
        </style>
    </head>
