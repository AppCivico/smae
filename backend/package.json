  "scripts": {
    "db:migrate:gen": "./bin/db-migrate-gen",
    "db:migrate:up": "prisma migrate dev",
    "db:migrate:down": "./bin/db-migrate-down",
    "start:migrate:watch": "PGSQL_DIR=./prisma/manual-copy/ node dist/bin/pgsql-migrate.js --watch",
    "db:reset": "prisma migrate reset --skip-generate",
    "db:seed": "prisma db seed",
    "db:generate": "prisma generate",
    "db:prod:deploy": "prisma migrate deploy",
    "organize-imports": "organize-imports-cli tsconfig.json",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"prisma/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "run-api.sh",
    "build:emails": "mjml src/public/email-src/emails/*.mjml -o src/public/email-templates/",
    "build:emails:watch": "mjml --config.validationLevel=strict -w src/public/email-src/emails/*.mjml -o src/public/email-templates/",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit --coverage",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --forceExit",
    "test:e2e:watch": "jest --config ./test/jest-e2e.json --watch",
    "test:e2e:cov": "jest --config ./test/jest-e2e.json --coverage --forceExit",
    "test:e2e:ci": "jest --config ./test/jest-e2e.json --coverage --forceExit --ci --reporters=default --reporters=jest-junit"
  },
